{
  "phases": {
    "package": {
      "context": {
        "resources": {
          "url": "{SHA_ARCHIVE}",
          "path": "/helm/sealed-secrets"
        }
      },
      "actions": [
        {
          "action_id": "helm-package"
        }
      ]
    },
    "verify": {
      "context": {
        "resources": {
          "url": "{SHA_ARCHIVE}",
          "path": "/.vib/"
        },
        "runtime_parameters": "",
        "target_platform": {
          "target_platform_id": "{TARGET_PLATFORM}"
        }
      },
      "actions": [
        {
          "action_id": "health-check",
          "params": {
            "endpoint": "lb-sealed-secrets-http"
          }
        }
      ]
    }
  }
}
